<%- include('./partials/header') %>
<h1><%= title %></h1>
<% if (book_list.length) { %>
<ul>
  <% for (const book of book_list) { %>
  <li>
    <a href="<%= book.url %>"><%= book.title %></a> <%= book.author.name %>
  </li>
  <% } %>
</ul>
<% } else { %>
<p>There are no books.</p>
<% } %> <%- include('./partials/footer') %>
